<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.web.project</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/web-project/3">
            <name>VendaProdutos</name>
            <minimum-ant-version>1.6.5</minimum-ant-version>
            <explicit-platform explicit-source-supported="true"/>
            <web-module-libraries>
                <library dirs="200">
                    <file>${libs.jakartaee-web-api-8.0.classpath}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library dirs="200">
                    <file>${libs.MariaDB-ConnectorJ-3.0.6.classpath}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
            </web-module-libraries>
            <web-module-additional-libraries/>
            <source-roots>
                <root id="src.dir"/>
            </source-roots>
            <test-roots>
                <root id="test.src.dir"/>
            </test-roots>
        </data>
        <libraries xmlns="http://www.netbeans.org/ns/ant-project-libraries/1">
            <definitions>.\lib\nblibraries.properties</definitions>
        </libraries>
        <libraries xmlns="http://www.netbeans.org/ns/cdnjs-libraries/1">
            <library name="decimal.js" version="10.3.1">
                <file localPath="web/js/libs/decimal.js/decimal.min.js" path="decimal.min.js"/>
            </library>
            <library name="jquery" version="3.6.0">
                <file localPath="web/js/libs/jquery/jquery.min.js" path="jquery.min.js"/>
            </library>
        </libraries>
    </configuration>
</project>
